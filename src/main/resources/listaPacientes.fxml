<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefWidth="402.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.clinica.controladores.ListaPacientesControlador">


    <Label text="Lista de Pacientes" textFill="#0f0f0f">
        <font>
            <Font size="20.0" />
        </font>
    </Label>


    <TableView fx:id="tablaPacientes" prefWidth="402.0" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn text="Identificación" />
            <TableColumn text="Nombre" />
            <TableColumn text="Teléfono" />
            <TableColumn text="Correo" />
            <TableColumn text="Suscripción" />
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        </columnResizePolicy>
    </TableView>
   <padding>
      <Insets bottom="20.0" />
   </padding>


</VBox>
